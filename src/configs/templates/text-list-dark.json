{
    "type": "APL",
    "version": "1.6",
    "__comment__": "Copyright 2021 Amazon.com, Inc. or its affiliates. All Rights Reserved.\nSPDX-License-Identifier: LicenseRef-.amazon.com.-AmznSL-1.0\nLicensed under the Amazon Software License  http://aws.amazon.com/asl/",
    "theme": "dark",
    "import": [
        {
            "name": "alexa-layouts",
            "version": "1.3.0"
        }
    ],
    "mainTemplate": {
        "parameters": [
            "payload"
        ],
        "items": [
            {
                "type": "AlexaTextList",
                "headerTitle": "${payload.textListData.title}",
                "headerBackButton": true,
                "headerAttributionImage": "${payload.textListData.logoUrl}",
                "backgroundImageSource": "${payload.textListData.backgroundImage.sources[0].url}",
                "listItems": "${payload.textListData.listItems}",
                "touchForward": true,
                "id": "plantList"
            }
        ]
    }
}
